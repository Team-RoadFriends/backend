<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.road_friends.rentalcar.mapper.ReservationMapper">

    <!-- 모든 예약 목록 조회 -->
    <select id="findAll" resultType="com.road_friends.rentalcar.dto.ReservationDto">
        SELECT reservation_s_id, reservation_s_create_date
        FROM reservation_s_data;
    </select>

    <!-- 특정 예약 조회-->
    <select id="findById" resultType="com.road_friends.rentalcar.dto.ReservationDto">
        SELECT reservation_s_id, car_id, reservation_s_start_date, reservation_s_end_date, reservation_s_create_date
        FROM reservation_s_data WHERE reservation_s_id = #{reservationSId};
    </select>
</mapper>