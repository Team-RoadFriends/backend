+package kr.or.ysedu.c702.service;
+
+import org.springframework.stereotype.Service;
+
+@Service
+public class MessageService {
+
+    public String choiceNum(int number){
+        if(number == 1){
+            return "common/about";
+        } else if (number == 2) {
+            return "common/contact";
+        } else if (number == 3) {
+            return "common/info";
+        }else {
+            return "common/404";
+        }
+    }
+}
